{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "make-a-gift",
  "version": "0.1.0",
  "identifier": "com.roomy.make-a-gift",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run dev"
  },
  "app": {
    "windows": [
      {
        "title": "make-a-gift",
        "width": 800,
        "height": 600,
        "resizable": false,
        "fullscreen": false,
        "url": "program.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "frameworks": [],
      "entitlements": null,
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null
    }
  }
}
